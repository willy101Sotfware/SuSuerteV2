<local:AppUserControl
    x:Class="SuSuerteV2.Presentation.UserControls.Chance.LoginChanceUC"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SuSuerteV2.UserControls"
    xmlns:local2="clr-namespace:SuSuerteV2.UserControls.ClassDiffrenceTextBox"
    xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="1920"
    d:DesignWidth="1080"
    mc:Ignorable="d">
    <Viewbox Stretch="Uniform">
        <Grid Width="1080" Height="1920">
            <Grid.Background>
                <ImageBrush ImageSource="/Images/Backgrounds/bgGray.png" />
            </Grid.Background>


            <TextBlock
                x:Name="tbTimer"
                Height="60"
                Margin="477,10,477,0"
                VerticalAlignment="Top"
                FontSize="45"
                Foreground="White" />

            <Image
                Width="92"
                Height="103"
                Margin="46,55,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                MouseDown="UIElement_Interaction"
                Source="/Images/Buttons/btnatras.png" />

            <Image
                Width="66"
                Height="99"
                Margin="206,719,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="/Images/Others/iconoCel.png" />

            <Image
                Height="46"
                Margin="380,751,378,0"
                VerticalAlignment="Top"
                Source="/Images/Others/txtCel.png" />


            <Image
                Width="670"
                Height="91"
                Margin="206,850,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="/Images/Others/input.png" />

            <local2:NumericBox
                x:Name="txtCelularForm"
                Margin="210,854,216,997"
                VerticalContentAlignment="Center"
                Background="{x:Null}"
                BorderBrush="{x:Null}"
                FontSize="40"
                FontWeight="Bold"
                IsEnabled="False"
                IsReadOnly="True"
                IsReadOnlyCaretVisible="False"
                Text=""
                TextAlignment="Center"
                TextWrapping="Wrap">
                <local2:NumericBox.Template>
                    <ControlTemplate />
                </local2:NumericBox.Template>
            </local2:NumericBox>


            <Image
                Width="85"
                Height="67"
                Margin="204,1031,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="/Images/Others/iconoMail.png" />

            <Image
                Height="45"
                Margin="364,1046,364,0"
                VerticalAlignment="Top"
                RenderTransformOrigin="0.659,0.644"
                Source="/Images/Others/Txt_Correo_Electronico.png" />


            <Image
                Width="670"
                Height="91"
                Margin="206,1124,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="/Images/Others/input.png" />

            <local2:EmailTextbox
                x:Name="txtEmailForm"
                Margin="210,1131,216,724"
                VerticalContentAlignment="Center"
                BorderBrush="{x:Null}"
                FontSize="40"
                FontWeight="Bold"
                IsReadOnly="True"
                IsReadOnlyCaretVisible="False"
                Text=""
                TextAlignment="Center"
                TextWrapping="Wrap" />

            <Image
                Width="69"
                Height="67"
                Margin="210,1257,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                MouseDown="UIElement_Interaction"
                Source="/Images/Buttons/BtnEditar.png"
                Tag="9"
                Visibility="Hidden" />

            <Label
                Margin="288,1269,676,584"
                Content="Editar"
                FontSize="35px"
                Foreground="White"
                Visibility="Hidden" />

            <TextBlock
                Margin="239,342,237,1257"
                FontSize="45"
                FontWeight="Bold"
                Foreground="White"
                Text="Confirma o actualiza los datos en pantalla, al correo electrónico llegará el comprobante de la apuesta a realizar"
                TextAlignment="Center"
                TextWrapping="Wrap" />

            <Image
                Width="291"
                Height="87"
                Margin="169,1487,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                MouseDown="UIElement_Interaction"
                RenderTransformOrigin="0.402,0.54"
                Source="/Images/Buttons/btnCancelar.png" />

            <Image
                Height="87"
                Margin="619,1487,170,0"
                VerticalAlignment="Top"
                MouseDown="UIElement_Interaction"
                Source="/Images/Buttons/btnContinuar.png" />

            <Image
                Width="588"
                Height="141"
                Margin="266,1769,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="/Images/Others/imgInfo.png" />
        </Grid>
    </Viewbox>
</local:AppUserControl>