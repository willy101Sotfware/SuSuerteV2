<local:AppUserControl
    x:Class="SuSuerteV2.Presentation.UserControls.Chance.FormUC"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SuSuerteV2.UserControls"
    xmlns:local2="clr-namespace:SuSuerteV2.UserControls.ClassDiffrenceTextBox"
    xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="1920"
    d:DesignWidth="1080"
    mc:Ignorable="d">
    <Viewbox Stretch="Uniform">
        <Grid Width="1080" Height="1920">
            <Grid.Background>
                <ImageBrush ImageSource="/Images/Backgrounds/BgRegistro.png" />
            </Grid.Background>

            <Grid.RowDefinitions>
                <RowDefinition Height="400" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <Grid Grid.Row="0">
                <TextBlock
                    x:Name="tbTimer"
                    Width="126"
                    Margin="0,0,0,341"
                    HorizontalAlignment="Center"
                    FontSize="45"
                    Foreground="White" />

                <!--  Botón Atrás  -->
                <Image
                    Width="92"
                    Height="102"
                    Margin="76,80,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Cursor="Hand"
                    PreviewMouseDown="HandleButtonAction"
                    Source="/Images/Buttons/btnatras.png"
                    Tag="Back"
                    TouchDown="HandleButtonAction" />

            </Grid>

            <Grid Grid.Row="1">

                <TextBlock
                    x:Name="txtNombre"
                    Width="388"
                    Margin="134,222,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Background="{x:Null}"
                    FontSize="30"
                    FontWeight="Bold"
                    Foreground="Black"
                    Text=""
                    TextAlignment="Left" />

                <TextBox
                    x:Name="txtSegundoNombre"
                    Margin="558,207,134,1243"
                    VerticalContentAlignment="Center"
                    Background="{x:Null}"
                    BorderBrush="{x:Null}"
                    FontSize="30"
                    FontWeight="Bold"
                    GotFocus="EventTxtSegundoNombre"
                    IsReadOnly="True"
                    SelectionBrush="{x:Null}"
                    TextAlignment="Left" />

                <TextBlock
                    x:Name="txtApellido"
                    Margin="134,374,558,1076"
                    VerticalAlignment="Center"
                    Background="{x:Null}"
                    FontSize="30"
                    FontWeight="Bold"
                    Text=""
                    TextAlignment="Left" />

                <TextBlock
                    x:Name="txtSegundoApellido"
                    Margin="558,374,134,1076"
                    VerticalAlignment="Center"
                    Background="{x:Null}"
                    FontSize="30"
                    FontWeight="Bold"
                    Text=""
                    TextAlignment="Left" />

                <TextBlock
                    x:Name="txtNumId"
                    Margin="558,540,134,910"
                    VerticalAlignment="Center"
                    Background="{x:Null}"
                    FontSize="30"
                    FontWeight="Bold"
                    Text=""
                    TextAlignment="Left" />
                <local2:NumericBox
                    x:Name="txtNumCel"
                    Margin="210,876,134,574"
                    VerticalContentAlignment="Center"
                    Background="{x:Null}"
                    BorderBrush="{x:Null}"
                    FontSize="30"
                    FontWeight="Bold"
                    IsReadOnly="True"
                    SelectionBrush="{x:Null}"
                    TextAlignment="Left"
                    TouchDown="TxtCel_Keyboard" />

                <local2:EmailTextbox
                    x:Name="txtEmaill"
                    Margin="210,1042,134,408"
                    VerticalContentAlignment="Center"
                    Background="{x:Null}"
                    BorderBrush="{x:Null}"
                    FontSize="30"
                    FontWeight="Bold"
                    IsReadOnly="True"
                    SelectionBrush="{x:Null}"
                    TextAlignment="Left"
                    TouchDown="TxtEmail_Keyboard" />


                <ComboBox
                    x:Name="fecha_dia"
                    Width="45"
                    Height="30"
                    Margin="568,730,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    FontSize="20"
                    FontWeight="Bold"
                    IsEnabled="False"
                    IsReadOnly="True" />

                <ComboBox
                    x:Name="fecha_mes"
                    Width="45"
                    Height="30"
                    Margin="630,730,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    FontSize="20"
                    FontWeight="Bold"
                    IsEnabled="False"
                    IsReadOnly="True" />

                <ComboBox
                    x:Name="fecha_ano"
                    Width="80"
                    Height="32"
                    Margin="689,728,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    FontSize="20"
                    FontWeight="Bold"
                    IsEnabled="False"
                    IsReadOnly="True" />

                <ComboBox
                    x:Name="typeDocument"
                    Width="388"
                    Height="74"
                    Margin="134,540,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    FontSize="30"
                    FontWeight="Bold"
                    IsEnabled="False"
                    IsReadOnly="True" />

                <ComboBox
                    x:Name="typeGenre"
                    Width="388"
                    Height="74"
                    Margin="134,707,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    FontSize="30"
                    FontWeight="Bold"
                    IsEnabled="False"
                    IsReadOnly="True" />



                <Image
                    x:Name="btnCheckDatos"
                    Margin="132,1141,908,329"
                    Source="{Binding imgPathCheck}"
                    TouchDown="BtnCheckTratamientoDatos" />


                <!--  Botón Cancelar  -->
                <Image
                    Width="302"
                    Height="81"
                    Margin="184,1306,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Cursor="Hand"
                    PreviewMouseDown="HandleButtonAction"
                    Source="/Images/Buttons/BtnCancelar.png"
                    Tag="Cancel"
                    TouchDown="HandleButtonAction" />

                <!--  Botón Continuar  -->
                <Image
                    Width="308"
                    Height="87"
                    Margin="590,1306,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Cursor="Hand"
                    PreviewMouseDown="HandleButtonAction"
                    Source="/Images/Buttons/btnContinuar.png"
                    Tag="Continue"
                    TouchDown="HandleButtonAction" />


            </Grid>


        </Grid>
    </Viewbox>
</local:AppUserControl>
