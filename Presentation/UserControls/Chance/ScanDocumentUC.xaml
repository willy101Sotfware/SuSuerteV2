<local:AppUserControl
    x:Class="SuSuerteV2.Presentation.UserControls.Chance.ScanDocumentUC"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:gif="http://wpfanimatedgif.codeplex.com"
    xmlns:local="clr-namespace:SuSuerteV2.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="1920"
    d:DesignWidth="1080"
    gif:ImageBehavior.AnimateInDesignMode="True"
    mc:Ignorable="d">
    <Viewbox Stretch="Uniform">
        <Grid Width="1080" Height="1920">
            <Grid.RowDefinitions>
                <RowDefinition Height="937*" />
                <RowDefinition Height="23*" />
            </Grid.RowDefinitions>
            <Grid.Background>
                <ImageBrush ImageSource="/Images/Backgrounds/BgSuSuerte.png" />
            </Grid.Background>


            <Image
                x:Name="GifLoadder"
                Grid.RowSpan="2"
                Width="906"
                Height="1524"
                Margin="86,400,88,-4"
                HorizontalAlignment="Center"
                VerticalAlignment="Top"
                gif:ImageBehavior.AnimateInDesignMode="True"
                gif:ImageBehavior.AnimatedSource="/Images/Gif/Escaner.gif"
                gif:ImageBehavior.AutoStart="True"
                Stretch="Fill" />



            <TextBlock
                x:Name="tbTimer"
                Height="60"
                Margin="477,10,477,0"
                VerticalAlignment="Top"
                FontSize="45"
                Foreground="White" />



            <Image
                Name="btnAtras"
                Width="92"
                Height="103"
                Margin="44,30,944,0"
                HorizontalAlignment="Center"
                VerticalAlignment="Top"
                MouseDown="UIElement_Interaction"
                Source="/Images/Buttons/btnatras.png" />




        </Grid>
    </Viewbox>
</local:AppUserControl>
